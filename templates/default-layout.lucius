h1, h2, h3, h4, h5, h6 {
    font-family: 'Corben', Georgia, Times, serif;
}

p, div {
    font-family: 'Cabin', Helvetica, Arial, sans-serif;
}

img.logo {
    width: 72px;
    padding-right: 10px;
}

hr {
    display: block;
    height: 1px;
    border: 0;
    border-top: 1px solid #ccc;
    margin: 1em 0;
    padding 0;
}

.container > hr {
    margin: 30px 0;
}
